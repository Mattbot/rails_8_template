{
  "name": "rails-8-template",
  "version": "1.0.3",
  "description": "Modern Rails 8 application template with esbuild, Slim templates, security scanning, and development automation",
  "private": true,
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --format=esm --outdir=app/assets/builds --public-path=/assets"
  },
  "devDependencies": {
    "esbuild": "^0.25.12"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.0",
    "@hotwired/turbo-rails": "^8.0.0"
  },
  "keywords": [
    "rails",
    "rails8",
    "template",
    "esbuild",
    "slim",
    "security",
    "development"
  ],
  "author": "Matt Ridenour <mattridenour@gmail.com>",
  "license": "MIT"
}
